{
  "rule": {
    "description": "Redirect www to non-www",
    "enabled": true,
    "expression": "http.host eq \"www.aromapulse.kr\"",
    "action": "redirect",
    "action_parameters": {
      "from_value": {
        "status_code": 301,
        "target_url": {
          "expression": "concat(\"https://aromapulse.kr\", http.request.uri.path)"
        },
        "preserve_query_string": true
      }
    }
  }
}
